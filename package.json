{
  "name": "jsonui",
  "version": "0.0.0",
  "repository": "git@github.com:fodori/jsonui.git",
  "author": "Istvan Fodor <fodori@jsonui.org>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "startReact": "yarn workspace @jsonui/react run start",
    "startCore": "yarn workspace @jsonui/core run start",
    "startTestReact": "yarn workspace jsonui-test-react run start",
    "publish": "yarn lerna publish from-package",
    "bootstrap": "yarn lerna bootstrap",
    "testCore": "yarn workspace @jsonui/core run test",
    "copyDocs": "rm -rf ./docs/*; cp -R ./packages/docs-site/build/* ./docs/;"
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "microbundle": "^0.14.2",
    "gh-pages": "^3.2.3"
  }
}
