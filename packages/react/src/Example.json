{
  "$comp": "View",
  "style": { "marginTop": 10, "backgroundColor": "orange" },
  "$children": [
    { "$comp": "Text", "$children": "JsonUI test page v0.1", "style": { "textAlign": "center", "fontSize": 30, "margin": 5 } },
    {
      "$comp": "Image",
      "style": { "width": 10, "height": 10 },
      "src": "data:image/png;base64, iVBORw0KGgoAAAANSUhEUgAAAAUAAAAFCAYAAACNbyblAAAAHElEQVQI12P4//8/w38GIAXDIBKE0DHxgljNBAAO9TXL0Y4OHwAAAABJRU5ErkJggg=="
    },
    {
      "component": "Fragment",
      "$isList": true,
      "$pathModifiers": {
        "data": { "path": "testList" }
      },
      "$page": { "$modifier": "get", "store": "data", "path": "../testTableActualPage" },
      "$itemPerPage": 5,
      "$listLength": { "$modifier": "get", "store": "data", "path": ".", "jsonataDef": "$count($)" },
      "test": { "$modifier": "test" },
      "$listItem": {
        "component": "Text",
        "children": { "$modifier": "get", "store": "data", "path": "address" }
      }
    }
  ]
}
